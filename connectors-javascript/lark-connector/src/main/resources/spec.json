                  "title": "${col_name}"
                },
                "properties": {
                  "name": {
                    "type": "string",
                    "required": true,
                    "x-decorator": "FormItem",
                    "x-component": "Input"
                  }
                }
              }
            }
          },
          "properties": {
            "add": {
              "type": "void",
              "x-component": "ArrayTable.Addition",
              "title": "${df_table_field_btn_add}"
            }
          }
        }
      }
    },
      "connection": {
        "type": "object",
        "properties": {
          "app_id": {
            "type": "String",
            "title": "${app_id}",
            "x-decorator": "FormItem",
            "x-component": "Input",
            "x-index": 10,
            "required": true
          },
          "app_secret": {
            "type": "String",
            "title": "${app_secret}",
            "x-decorator": "FormItem",
            "x-component": "Input",
            "x-index": 20,
            "required": true
          }
        }
      }
    },
    "messages": {
      "default": "en_US",
